.scoreBox{
  width:100%;
  height: 120rpx;
  background: #FFFFFF;
  display:flex;
  align-items:center;
  justify-content:space-between;
  margin:20rpx 0;
}

.scoreBoxL{
  margin-left:30rpx;
}

.scoreBoxL text:nth-child(1){
  font-size: 32rpx;
  font-weight: bold;
  color: #FF5901;
}

.scoreBoxL text:nth-child(2){
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
  margin-left:10rpx;
}

.scoreBoxR{
  margin-right:30rpx;
  width: 78rpx;
  height: 38rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FF5901;
  text-align: center;
  line-height:38rpx;
  background: #FFFFFF;
  border-radius: 30rpx;
  border: 2rpx solid #FF5901;
}

/* listShow */
.listShow{
  width:692rpx;
  margin:30rpx auto;
}

/* 列表 */
.footlist {
  width:100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  display:flex;
  justify-content:space-between;
}

.foot-left{
  width:336rpx;
}

.foot-right{
  width:336rpx;
}

.footbox {
  width:336rpx;
  margin-bottom:20rpx;
  background:#fff;
  border-radius:30rpx;
}

.footImg {
  width:336rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.footbox .t1 {
  font-size: 28rpx;
  font-weight: 400;
  color: #222B45;
  line-height:26rpx;
  overflow: hidden;
}


.box-shadow {
  position:relative;
  background: #fff;
  padding:8rpx 16rpx 14rpx;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}

.ziYing{
  width: 52rpx;
  height: 30rpx;
  font-size: 20rpx;
  font-weight: 500;
  color:#fff;
  background: #FF5901;
  border-radius: 4rpx;
  text-align:center;
  line-height:30rpx;
  position:absolute;
  top:20rpx;
  left:20rpx;
}

.footPriceBox{
  width:100%;
  display:flex;
  align-items:center;
  justify-content: space-between;
  margin-top:10rpx;
}

.carImg{
  width:36rpx;
  height:38rpx;
}

.footPrice{
  font-size:24rpx;
  font-weight: 600;
  color: #ED1111;
}