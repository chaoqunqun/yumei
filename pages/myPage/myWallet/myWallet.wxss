page{
  background:#fff;
}

.walletTop{
  width:100%;
  height:300rpx;
  display:flex;
  flex-direction: column;
  align-items:center;
  justify-content: center;
  background:#FF5901;
}

.walletTop view:nth-child(1){
  font-size: 92rpx;
  font-weight: bold;
  color: #FFFFFF;
}

.walletTop view:nth-child(2){
  font-size: 24rpx;
  font-weight: 500;
  color: #FECCB4;
}

.scoreNav{
  width:690rpx;
  height: 120rpx;
  display:flex;
  align-items:center;
  justify-content: space-between;
  background: #FFFFFF;
  border-bottom: 2rpx solid #F0F3F5;
  margin:0 auto;
}

.scoreNav:last-child{
  border:none;
}

.scoreTime{
  font-size: 32rpx;
  font-weight: 500;
  color: #222B45;
}

.duiImg{
  width:40rpx;
  height:40rpx;
}