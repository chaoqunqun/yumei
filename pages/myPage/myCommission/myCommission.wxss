page{
  background:#fff;
}

.bg{
  width:100%;
  height:20rpx;
  background:#f8f8f8;
}

.comsinBox{
  width: 100%;
  height: 159px;
  overflow: hidden;
}

.comsinBoxSon{
  width:690rpx;
  height:188rpx;
  margin:0 auto;
  overflow: hidden;
  border-bottom:2rpx solid #F0F3F5;
}

.comsinTop{
  width:100%;
  height:94rpx;
  margin-top:20rpx;
  display:flex;
  align-items:center;
  justify-content: space-between;
}

.comsionPrice{
  font-size: 80rpx;
  font-weight: 600;
  color: #FF5901;
}

.comsionD{
  display:flex;
  align-items:center;
}

.comsionD text{
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
}

.wenImg{
  width:36rpx;
  height:36rpx;
  margin-left:16rpx;
}

.taxText{
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
  margin-top:6rpx;
}

.comisonBom{
  width:100%;
  display:flex;
  align-items:center;
  justify-content: space-between;
}

.comisonBomSon{
  width:610rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  margin-top:28rpx;
}

.comisonBomSon view:nth-child(1){
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
  margin-bottom:4rpx;
}

.comisonBomSon view:nth-child(2){
  font-size: 28rpx;
  font-weight: 600;
  color: #222B45;
}

/* top */
.top{
  width:100%;
  height:92rpx;
  background:#f8f8f8;
  overflow: hidden;
  display:flex;
  justify-content:space-between;
}

.topSon{
  width: 375rpx;
  height: 92rpx;
  display:flex;
  flex-direction:column;
  align-items:center;
}

.topSonP{
  height:24rpx;
}

.topText{
  font-size: 28rpx;
  font-weight: 400;
  color: #8894A4;
}

.hengLine{
  width: 32rpx;
  height: 6rpx;
  background: #FF5901;
  border-radius: 4rpx;
  margin-top:20rpx;
}

.activeText{
  font-size: 32rpx;
  font-weight: 500;
  color: #222B45;
}

/* 积分明细 */
.scoreBox{
  width:690rpx;
  height:140rpx;
  display:flex;
  align-items:center;
  justify-content: space-between;
  border-bottom: 2rpx solid #F0F3F5;
  margin:0 auto;
  position: relative;
}

.scoreBoxL view:nth-child(1){
  font-size: 32rpx;
  font-weight: bold;
  color: #FF5901;
  margin-bottom:6rpx;
}

.scoreBoxL view:nth-child(2){
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
}

.scoreBoxR{
  display:flex;
  flex-direction: column;
  align-items: flex-end;
}

.scoreBoxR view:nth-child(1){
  font-size: 24rpx;
  font-weight: 400;
  color: #ABA8A6;
  margin-bottom:6rpx;
}

.scoreBoxR view:nth-child(2){
  font-size: 24rpx;
  font-weight: 400;
  color: #222B45;
}

.comsionType{
  position: absolute;
  bottom:30rpx;
  right:214rpx;
}

.comsionType text:nth-child(1){
  font-size: 24rpx;
  font-weight: 400;
  color: #C4C4C4;
}

.comsionType text:nth-child(2){
  font-size: 24rpx;
  font-weight: 400;
  color: #222B45;
}